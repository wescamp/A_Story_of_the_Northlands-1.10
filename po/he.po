msgstr ""

#. [unit]: id=Doc, type=Brute
#: A_Story_of_the_Northlands/scenarios/01_Prologue.cfg:132
msgid "Doc"
msgstr ""

#. [side]: type=Goblin Pillager, id=Badush
#: A_Story_of_the_Northlands/scenarios/01_Prologue.cfg:174
msgid "Badush"
msgstr ""

#. [side]
#. [side]: type=Direwolf Rider
#. [side]: type=Goblin Pillager, id=Badush
#. [side]: type=Orcish Ruler, id=ruler
#. [side]: type=Orcish Slurbow
#. [side]: type=Orcish Sovereign, id=chief
#. [side]: type=Orcish Sovereign, id=ruler
#. [side]: type=Orcish Warlord
#. [side]: type=Orcish Warlord, id=Eradash
#: A_Story_of_the_Northlands/scenarios/01_Prologue.cfg:181
#: A_Story_of_the_Northlands/scenarios/01_Prologue.cfg:188
#: A_Story_of_the_Northlands/scenarios/05_Shadows.cfg:92
#: A_Story_of_the_Northlands/scenarios/07_Ambush.cfg:105
#: A_Story_of_the_Northlands/scenarios/08_Charge.cfg:142
#: A_Story_of_the_Northlands/scenarios/09_Revenge.cfg:94
#: A_Story_of_the_Northlands/scenarios/09_Revenge.cfg:105
